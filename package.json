{
  "name": "thibmaek.com",
  "description": "A nice blog about development, Raspberry Pi, plants and probably records",
  "version": "2.1.0",
  "author": "Thibault Maekelbergh",
  "license": "MIT",
  "engines": {
    "node": ">=8",
    "npm": ">=5"
  },
  "scripts": {
    "build": "gatsby build",
    "clean:build": "rm -rf public && npm run build",
    "clean:deps": "rm -rf node_modules",
    "clean:install": "npm run clean:deps && npm install",
    "eslint:fix": "npm run eslint -- --fix",
    "eslint": "eslint \"src/**/*.js\"",
    "format": "prettier --trailing-comma es5 --no-semi --single-quote --write \"src/**/*.js\"",
    "lint": "npm run eslint && npm run stylelint",
    "packages:update": "npm-check -u -E",
    "precommit": "lint-staged",
    "serve": "serve public/",
    "start": "gatsby develop",
    "stylelint:fix": "npm run stylelint -- --fix",
    "stylelint": "stylelint \"src/**/*.css\"",
    "test:ci": "npm run lint"
  },
  "devDependencies": {
    "dotenv": "6.0.0",
    "eslint": "5.5.0",
    "eslint-config-thibmaek": "1.0.4",
    "eslint-plugin-babel": "5.1.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-react": "7.11.1",
    "husky": "^0.14.3",
    "lint-staged": "7.2.2",
    "npm-check": "5.8.0",
    "prettier": "1.14.2",
    "serve": "10.0.0",
    "stylelint": "9.5.0",
    "stylelint-config-devine": "1.1.6",
    "stylelint-order": "1.0.0"
  },
  "dependencies": {
    "date-fns": "^1.29.0",
    "gatsby": "1.9.277",
    "gatsby-link": "1.6.46",
    "gatsby-paginate": "1.0.16",
    "gatsby-plugin-canonical-urls": "^1.0.18",
    "gatsby-plugin-feed": "1.3.25",
    "gatsby-plugin-netlify": "^1.0.21",
    "gatsby-plugin-no-sourcemaps": "^1.0.5",
    "gatsby-plugin-offline": "1.0.18",
    "gatsby-plugin-react-helmet": "2.0.11",
    "gatsby-remark-emoji": "0.0.2",
    "gatsby-remark-external-links": "0.0.4",
    "gatsby-remark-prismjs": "2.0.5",
    "gatsby-source-contentful": "1.3.54",
    "gatsby-transformer-remark": "1.7.44",
    "isomorphic-fetch": "^2.2.1",
    "normalize.css": "8.0.0",
    "prismjs": "1.15.0",
    "prop-types": "15.6.2",
    "react": "16.4.2",
    "react-disqus-comments": "1.2.0",
    "react-helmet": "^5.2.0"
  }
}

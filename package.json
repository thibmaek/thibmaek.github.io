{
  "name": "blog.thibmaekelbergh.be",
  "description": "A nice blog about development, Raspberry Pi, plants and probably records",
  "version": "1.2.0",
  "author": "Thibault Maekelbergh",
  "license": "MIT",
  "main": "",
  "engines": {
    "node": ">=8"
  },
  "scripts": {
    "build": "gatsby build",
    "clean:build": "rm -rf public && npm run build",
    "clean:deps": "rm -rf node_modules",
    "clean:install": "npm run clean:deps && npm install",
    "eslint:fix": "npm run eslint -- --fix",
    "eslint": "eslint \"src/**/*.js\"",
    "format": "prettier --trailing-comma es5 --no-semi --single-quote --write \"src/**/*.js\"",
    "lint": "npm run eslint && npm run stylelint",
    "packages:update": "npm-check -u -E",
    "precommit": "lint-staged",
    "serve": "serve public/",
    "start": "gatsby develop",
    "stylelint:fix": "npm run stylelint -- --fix",
    "stylelint": "stylelint \"src/**/*.css\"",
    "test:ci": "npm run lint"
  },
  "devDependencies": {
    "dotenv": "5.0.1",
    "eslint": "4.19.1",
    "eslint-config-thibmaek": "1.0.4",
    "eslint-plugin-babel": "5.1.0",
    "eslint-plugin-react": "7.8.2",
    "husky": "^0.14.3",
    "lint-staged": "7.1.1",
    "npm-check": "5.7.1",
    "prettier": "1.12.1",
    "serve": "^6.5.7",
    "stylelint": "9.2.1",
    "stylelint-config-devine": "1.1.6",
    "stylelint-order": "0.8.1"
  },
  "dependencies": {
    "date-fns": "^1.29.0",
    "gatsby": "1.9.261",
    "gatsby-link": "1.6.44",
    "gatsby-paginate": "1.0.14",
    "gatsby-plugin-feed": "1.3.22",
    "gatsby-plugin-offline": "1.0.16",
    "gatsby-plugin-react-helmet": "2.0.11",
    "gatsby-remark-emoji": "0.0.2",
    "gatsby-remark-external-links": "0.0.4",
    "gatsby-remark-prismjs": "2.0.2",
    "gatsby-source-contentful": "1.3.51",
    "gatsby-transformer-remark": "1.7.41",
    "isomorphic-fetch": "^2.2.1",
    "normalize.css": "8.0.0",
    "prismjs": "1.14.0",
    "prop-types": "15.6.1",
    "react": "16.3.2",
    "react-disqus-comments": "1.2.0",
    "react-helmet": "^5.2.0"
  }
}

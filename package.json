{
  "name": "thibmaek.com",
  "description": "A nice blog about development, Raspberry Pi, plants and probably records",
  "version": "2.1.0",
  "author": "Thibault Maekelbergh",
  "license": "MIT",
  "engines": {
    "node": ">=8",
    "npm": ">=5"
  },
  "scripts": {
    "build": "gatsby build",
    "clean:build": "rm -rf public && npm run build",
    "clean:deps": "rm -rf node_modules",
    "clean:install": "npm run clean:deps && npm install",
    "eslint:fix": "npm run eslint -- --fix",
    "eslint": "eslint \"src/**/*.js\"",
    "format": "prettier --trailing-comma es5 --no-semi --single-quote --write \"src/**/*.js\"",
    "lint": "npm run eslint && npm run stylelint",
    "update": "npm-check -u --production",
    "update:dev": "npm-check -u --dev-only",
    "precommit": "lint-staged",
    "serve": "serve public/",
    "start": "gatsby develop",
    "stylelint:fix": "npm run stylelint -- --fix",
    "stylelint": "stylelint \"src/**/*.css\"",
    "test:ci": "npm run lint"
  },
  "devDependencies": {
    "dotenv": "6.0.0",
    "eslint": "4.19.1",
    "eslint-config-thibmaek": "1.0.4",
    "eslint-plugin-babel": "5.1.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-react": "7.9.1",
    "husky": "^0.14.3",
    "lint-staged": "7.1.3",
    "npm-check": "5.7.1",
    "prettier": "1.13.5",
    "serve": "8.1.2",
    "stylelint": "9.2.1",
    "stylelint-config-devine": "1.1.6",
    "stylelint-order": "0.8.1"
  },
  "dependencies": {
    "classnames": "^2.2.6",
    "date-fns": "^1.29.0",
    "gatsby": "^2.9.11",
    "gatsby-link": "1.6.44",
    "gatsby-paginate": "1.0.16",
    "gatsby-plugin-canonical-urls": "^1.0.18",
    "gatsby-plugin-feed": "1.3.24",
    "gatsby-plugin-netlify": "^1.0.21",
    "gatsby-plugin-no-sourcemaps": "^1.0.5",
    "gatsby-plugin-offline": "1.0.18",
    "gatsby-plugin-react-helmet": "2.0.11",
    "gatsby-remark-emoji": "0.0.2",
    "gatsby-remark-external-links": "0.0.4",
    "gatsby-remark-prismjs": "2.0.3",
    "gatsby-source-contentful": "1.3.52",
    "gatsby-transformer-remark": "1.7.42",
    "isomorphic-fetch": "^2.2.1",
    "normalize.css": "8.0.0",
    "prismjs": "1.14.0",
    "prop-types": "15.6.1",
    "react": "16.4.0",
    "react-helmet": "^5.2.0",
    "react-toggle": "^4.0.2"
  }
}
